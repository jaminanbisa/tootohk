<?xml version="1.0" encoding="utf-8"?>
<bundle name="PublishPress Permissions Pro">
  <domain name="presspermit-pro">
    <project name="PublishPress Permissions Pro" slug="presspermit-pro">
      <source>
        <directory>.</directory>
      </source>
      <target>
        <directory>languages</directory>
      </target>
      <template>
        <file>languages/presspermit-pro.pot</file>
      </template>
    </project>
  </domain>
  <domain name="presspermit-pro-hints">
    <project name="PublishPress Permissions Pro Hints" slug="presspermit-pro-hints">
      <source>
        <file>includes-pro/admin-includes.php</file>
      </source>
      <target>
        <directory>languages</directory>
      </target>
      <template>
        <file>languages/presspermit-pro-hints.pot</file>
      </template>
    </project>
  </domain>
  <domain name="press-permit-core">
    <project name="PublishPress Permissions" slug="press-permit-core">
      <source>
        <directory>vendor/publishpress/publishpress-permissions</directory>
      </source>
      <target>
        <directory>vendor/publishpress/publishpress-permissions/languages</directory>
      </target>
      <template>
        <file>vendor/publishpress/publishpress-permissions/languages/press-permit-core.pot</file>
      </template>
    </project>
  </domain>
  <domain name="press-permit-core-hints">
    <project name="PublishPress Permissions Hints" slug="press-permit-core-hints">
      <source>
        <file>vendor/publishpress/publishpress-permissions/classes/PublishPress/Permissions/UI/SettingsAdmin.php</file>
		    <file>vendor/publishpress/publishpress-permissions/classes/PublishPress/Permissions/Constants.php</file>
        <file>modules/presspermit-import/classes/Permissions/Import/UI/SettingsTabImportNotes.php</file>
      </source>
      <target>
        <directory>vendor/publishpress/publishpress-permissions/languages</directory>
      </target>
      <template>
        <file>vendor/publishpress/publishpress-permissions/languages/press-permit-core-hints.pot</file>
      </template>
    </project>
  </domain>
  <exclude>
    <directory>vendor</directory>
    <directory>vendor/publishpress/publishpress-permissions/vendor</directory>
	  <file>vendor/publishpress/publishpress-permissions/classes/PublishPress/Permissions/UI/SettingsAdmin.php</file>
	  <file>vendor/publishpress/publishpress-permissions/classes/PublishPress/Permissions/Constants.php</file>
    <file>modules/presspermit-import/classes/Permissions/Import/UI/SettingsTabImportNotes.php</file>
    <file>includes-pro/admin-includes.php</file>
  </exclude>
</bundle>
